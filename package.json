{
   "name": "e-lab",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start -p 4000",
      "deploy": "node server.js",
      "test": "npx jest",
      "gen:db": "ts-node ./src/database/generate/index.ts",
      "gen:keypair": "node generateKeypair.js",
      "lint": "next lint",
      "typeorm": "typeorm-ts-node-commonjs",
      "db:setup": "yarn db:create:dev && yarn db:create:test",
      "db:create:dev": "createdb --owner=admin elabdev",
      "db:create:test": "createdb --owner=admin elabtest",
      "g:migration": "yarn typeorm:local migration:generate -n",
      "g:entity": "yarn typeorm:local entity:create -n",
      "typeorm:local": "yarn local ./node_modules/typeorm/cli.js",
      "local": "DOTENV_CONFIG_PATH=./.env ts-node -P ./tsconfig.yarn.json -r dotenv/config"
   },
   "dependencies": {
      "apollo-server": "^3.12.1",
      "autoprefixer": "10.4.14",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "compression": "^1.7.4",
      "cookie": "^0.5.0",
      "cross-env": "^7.0.3",
      "crypto": "^1.0.1",
      "dotenv": "^16.0.3",
      "eslint": "8.39.0",
      "eslint-config-next": "13.3.1",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.2",
      "moment": "^2.29.4",
      "next": "13.3.1",
      "nodemailer": "^6.9.4",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "pg": "^8.4.0",
      "pg-pool": "^3.6.1",
      "postcss": "8.4.23",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "redis": "^4.6.8",
      "reflect-metadata": "^0.1.13",
      "slugify": "^1.6.6",
      "tailwindcss": "3.3.1",
      "type-graphql": "^1.1.1",
      "typedi": "^0.10.0",
      "typeorm": "0.3.15",
      "util": "^0.12.5",
      "uuidv4": "^6.2.13"
   },
   "devDependencies": {
      "@babel/core": "^7.22.10",
      "@babel/preset-env": "^7.22.10",
      "@jest-decorated/core": "^0.1.7",
      "@types/cookie": "^0.5.1",
      "@types/jest": "^29.5.3",
      "@types/node": "^18.16.3",
      "@types/nodemailer": "^6.4.9",
      "@types/pg": "^8.6.6",
      "@types/react": "^18.2.0",
      "@types/react-dom": "^18.2.1",
      "babel-jest": "^29.6.2",
      "chance": "^1.1.11",
      "jest": "^29.6.2",
      "jest-mock-extended": "^3.0.5",
      "prettier": "^2.8.8",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.0.4"
   }
}
